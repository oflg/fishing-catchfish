title: Excerpt

<$list
    filter="[<currentTiddler>tags[]![?]![!]]"
    variable="sourceTiddler"
>
    {{{ [{!!title}split[/]last[]] }}}
    <span
        class="tc-fish-title"
    >
        <$dynannotate
            search={{!!annotate-text}}
            searchDisplay="snippet"
        >
            <$transclude
                tiddler=<<sourceTiddler>>
                mode="block"
            />
        </$dynannotate>
    </span>
</$list>
<$list
    filter="[<currentTiddler>tags[]![?]![!]] +[count[]match[0]]"
    variable="null"
>
    {{{ [{!!title}split[/]last[]] }}}
</$list>
